	if test "bzImage.gz" = "bzImage"; then
		gzip -9c < "bzImage" > "arch/k1om/boot/bzImage"
	fi

}

oe_runmake() {
	if [ x"$MAKE" = x ]; then MAKE=make; fi
	bbnote make -j 16  "$@"
	make -j 16  "$@" || die "oe_runmake failed"

}

die() {
